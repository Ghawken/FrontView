<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Controls="clr-namespace:FluidKit.Controls">
	<Style TargetType="Controls:TransitionPresenter">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="Controls:TransitionPresenter">
					<Grid>
						<Grid x:Name="PART_ChildrenContainer" IsItemsHost="True" />
						<Grid x:Name="PART_TransitionElementsContainer" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
</ResourceDictionary>